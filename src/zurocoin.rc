#include "version.hpp"
/////////////////////////////////////////////////////////////////////////////
//
// Version
//

1 VERSIONINFO
 FILEVERSION zurocoin_VERSION_WINDOWS_COMMA
 PRODUCTVERSION zurocoin_VERSION_WINDOWS_COMMA
 FILEFLAGSMASK 0x3fL
 FILEFLAGS 0x0L
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Zurocoin Developers"
            VALUE "FileDescription", "Zurocoin"
            VALUE "FileVersion", zurocoin_VERSION_STRING
            VALUE "InternalName", "Zurocoin"
            VALUE "LegalCopyright", "Copyright (C) Zurocoin Developers 2012-2018"
            VALUE "OriginalFilename", "Zurocoin"
            VALUE "ProductName", "Zurocoin"
            VALUE "ProductVersion", zurocoin_VERSION_STRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

// Icon with lowest ID value placed first to ensure application icon
// remains consistent on all systems.
101               ICON                    "zurocoin.ico"
